{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/about/?$",
			"dest": "_render"
		},
		{
			"src": "^/contact/?$",
			"dest": "_render"
		},
		{
			"src": "^/customer/?$",
			"dest": "_render"
		},
		{
			"src": "^/disclaimer/?$",
			"dest": "_render"
		},
		{
			"src": "^/pnp/?$",
			"dest": "_render"
		},
		{
			"src": "^/product/?$",
			"dest": "_render"
		},
		{
			"src": "^/tnc/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	],
	"images": {
		"sizes": [
			640,
			750,
			828,
			1080,
			1200,
			1920
		],
		"domains": [],
		"minimumCacheTTL": 60,
		"remotePatterns": []
	}
}